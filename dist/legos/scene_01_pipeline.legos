title: "Shield Production Pipeline & Triage Strategy"
description: "Master overview showing the transformation from Brainstorming/Chaos to Execution/Order through three tribes: Humanists, Techs, and Designers"
location: "forge_command_center"

entities:
  - id: "e_humanists"
    type: "tribe"
    name: "The Humanists"
    traits: ["ontological", "scholarly", "interpretive"]
    role: "Ontology Engineers - Define world logic"
    location: "triage_station"
  - id: "e_techs"
    type: "tribe"
    name: "The Techs"
    traits: ["technical", "pipeline-focused", "material"]
    role: "Pipeline Builders - Process museum data to LoRA"
    location: "triage_station"
  - id: "e_designers"
    type: "tribe"
    name: "The Designers"
    traits: ["spatial", "visual", "world-building"]
    role: "World Builders - Construct 3D environments"
    location: "triage_station"
  - id: "e_schema"
    type: "artifact"
    name: "Schema (Alpha)"
    traits: ["structured", "machine-readable"]
    location: "output_zone"
  - id: "e_substrate"
    type: "artifact"
    name: "Substrate (Beta)"
    traits: ["textured", "bronze-authentic"]
    location: "output_zone"
  - id: "e_logic_interface"
    type: "artifact"
    name: "Logic & Spatial Interface (Gold)"
    traits: ["navigable", "spatially-defined"]
    location: "output_zone"
  - id: "e_auden_button"
    type: "mechanism"
    name: "Auden Button"
    traits: ["toggle", "critique-layer"]
    location: "output_zone"
  - id: "e_delivered_artifact"
    type: "artifact"
    name: "Delivered Artifact"
    traits: ["complete", "shield-ready"]
    location: "execution_zone"

goals:
  - id: "g_transform_chaos"
    name: "Transform Brainstorming/Chaos into Execution/Order"
    owner: "e_humanists"
  - id: "g_produce_schema"
    name: "Produce machine-readable ontology schema"
    owner: "e_humanists"
  - id: "g_produce_substrate"
    name: "Produce authentic bronze material substrate"
    owner: "e_techs"
  - id: "g_produce_spatial"
    name: "Produce navigable 3D spatial interface"
    owner: "e_designers"
  - id: "g_deliver_shield"
    name: "Deliver complete Shield artifact"
    owner: "e_humanists"

obstacles:
  - id: "o_chaos"
    name: "Unstructured brainstorming scattered across domains"
    affects: "g_transform_chaos"
  - id: "o_hallucination"
    name: "AI hallucinating anachronistic elements"
    affects: "g_produce_substrate"
  - id: "o_scope_creep"
    name: "Scope creep threatening spatial consistency"
    affects: "g_produce_spatial"

shifts:
  - id: "s_triage"
    name: "Triage prioritization gates work into P0/P1/P2/P3"
    causes: ["o_chaos"]
    results_in: "g_transform_chaos"
  - id: "s_schema_complete"
    name: "Schema Alpha deliverable complete"
    causes: []
    results_in: "g_produce_schema"
  - id: "s_substrate_validated"
    name: "Substrate passes Bronze Phone test"
    causes: ["o_hallucination"]
    results_in: "g_produce_substrate"
  - id: "s_spatial_locked"
    name: "Spatial interface locked at human scale"
    causes: ["o_scope_creep"]
    results_in: "g_produce_spatial"

relations:
  - "[produces] e_humanists -> e_schema"
  - "[produces] e_techs -> e_substrate"
  - "[produces] e_designers -> e_logic_interface"
  - "[combines] e_schema + e_substrate + e_logic_interface -> e_auden_button"
  - "[gates] e_auden_button -> e_delivered_artifact"

timeline:
  - id: "t_pipeline"
    description: "Production Pipeline Flow"
    scenes:
      - id: "sc_triage"
        description: "Triage station receives chaotic input"
        entities: ["e_humanists", "e_techs", "e_designers"]
      - id: "sc_parallel_work"
        description: "Three tribes work in parallel streams"
        entities: ["e_schema", "e_substrate", "e_logic_interface"]
      - id: "sc_convergence"
        description: "Outputs converge at Auden Button"
        entities: ["e_auden_button"]
      - id: "sc_delivery"
        description: "Final artifact delivered"
        entities: ["e_delivered_artifact"]

