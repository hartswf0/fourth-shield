title: "Work Stream 1: The Humanists - Essays to Data"
description: "Transforming scholarly prose into machine-readable structured data via the Monday Morning Plan"
location: "humanist_office"

entities:
  - id: "e_scholar_prose"
    type: "input"
    name: "Scholarly Essays and Prose"
    traits: ["verbose", "interpretive", "narrative"]
    location: "input_pile"
  - id: "e_schema_alpha"
    type: "artifact"
    name: "Schema (Alpha)"
    traits: ["structured", "JSON", "machine-readable"]
    location: "output"
  - id: "e_classicist"
    type: "role"
    name: "The Classicist"
    traits: ["expert", "interpretive", "scholarly"]
    location: "humanist_office"
  - id: "e_csv_request"
    type: "action"
    name: "Email request for CSV data"
    traits: ["specific", "bounded", "deliverable"]
    location: "communication"
  - id: "e_shield_ontology"
    type: "artifact"
    name: "shield_ontology.json"
    traits: ["complete", "validated", "integrated"]
    location: "final_output"

goals:
  - id: "g_transform_prose"
    name: "Transform prose descriptions to structured data"
    owner: "e_humanists"
  - id: "g_force_decisions"
    name: "Force interpretive decisions into machine-readable form"
    owner: "e_classicist"
  - id: "g_prevent_hallucination"
    name: "Ensure historical accuracy coded into logic"
    owner: "e_humanists"

obstacles:
  - id: "o_vague_prose"
    name: "Prose too vague for simulation engine"
    affects: "g_transform_prose"
  - id: "o_no_structure"
    name: "No inherent structure in narrative descriptions"
    affects: "g_force_decisions"

shifts:
  - id: "s_request_csv"
    name: "Email request for specific plants/tools from Iliad 18"
    causes: ["o_no_structure"]
    results_in: "g_force_decisions"
  - id: "s_produce_schema"
    name: "Produce JSON schema from structured CSV"
    causes: ["o_vague_prose"]
    results_in: "g_transform_prose"

relations:
  - "[requests] e_classicist -> e_csv_request"
  - "[transforms] e_csv_request -> e_scholar_prose"
  - "[produces] e_classicist -> e_schema_alpha"
  - "[integrates] e_schema_alpha -> e_shield_ontology"

timeline:
  - id: "t_humanist_stream"
    description: "Humanist Work Stream"
    scenes:
      - id: "sc_problem"
        description: "THE PROBLEM: Narrative prose not machine-readable"
        entities: ["e_scholar_prose"]
      - id: "sc_solution"
        description: "THE SOLUTION: Force structured data output"
        entities: ["e_schema_alpha"]
      - id: "sc_action"
        description: "MONDAY MORNING: Email Classicist for CSV"
        entities: ["e_classicist", "e_csv_request"]
      - id: "sc_deliverable"
        description: "THE DELIVERABLE: shield_ontology.json"
        entities: ["e_shield_ontology"]

