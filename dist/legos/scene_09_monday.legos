title: "Monday Morning Plan: Triage Execution & Next Steps"
description: "Summary of immediate actions, email briefs, and decisions for team alignment across all three work streams"
location: "command_center"

entities:
  - id: "e_classicist"
    type: "role"
    name: "The Classicist"
    traits: ["scholarly", "data-structuring", "csv-focused"]
    location: "humanist_station"
  - id: "e_dev"
    type: "role"
    name: "The Dev"
    traits: ["technical", "material-focused", "texture-expert"]
    location: "tech_station"
  - id: "e_director"
    type: "role"
    name: "The Director"
    traits: ["spatial", "decision-maker", "camera-stance"]
    location: "design_station"
  - id: "e_csv_deliverable"
    type: "artifact"
    name: "Structured CSV File"
    traits: ["plants", "tools", "Iliad-18"]
    location: "output"
  - id: "e_texture_assets"
    type: "artifact"
    name: "Realistic Bronze Texture Assets"
    traits: ["roughness", "metallic", "validated"]
    location: "output"
  - id: "e_world_context"
    type: "artifact"
    name: "Immersive World Context"
    traits: ["camera-defined", "participant-view"]
    location: "output"
  - id: "e_ontology_lead"
    type: "job_card"
    name: "Ontology Lead Job Card"
    traits: ["schema-authoring", "graph-interpretation"]
    location: "formal_invitations"
  - id: "e_pipeline_engineer"
    type: "job_card"
    name: "Pipeline Engineer Job Card"
    traits: ["lora-training", "material-validation"]
    location: "formal_invitations"
  - id: "e_world_builder"
    type: "job_card"
    name: "World Builder Job Card"
    traits: ["unreal-engine", "spatial-design"]
    location: "formal_invitations"

goals:
  - id: "g_email_classicist"
    name: "Email Classicist for CSV of plants/tools by Friday"
    owner: "e_classicist"
  - id: "g_email_dev"
    name: "Email Dev brief focusing on roughness/metallic, not animation"
    owner: "e_dev"
  - id: "g_define_camera"
    name: "Define camera stance: walking on shield, not flying over"
    owner: "e_director"
  - id: "g_generate_job_cards"
    name: "Generate formal job description cards for each role"
    owner: "e_director"

obstacles:
  - id: "o_animation_distraction"
    name: "Dev distracted by animation instead of materials"
    affects: "g_email_dev"
  - id: "o_observer_confusion"
    name: "Unclear if user flies or walks"
    affects: "g_define_camera"

shifts:
  - id: "s_csv_request"
    name: "Email request for specific data points"
    causes: []
    results_in: "g_email_classicist"
  - id: "s_focus_textures"
    name: "Stop animation attempts, focus on textures"
    causes: ["o_animation_distraction"]
    results_in: "g_email_dev"
  - id: "s_lock_camera"
    name: "Decision: We are walking on it"
    causes: ["o_observer_confusion"]
    results_in: "g_define_camera"

relations:
  - "[emails] e_classicist -> e_csv_deliverable"
  - "[emails] e_dev -> e_texture_assets"
  - "[defines] e_director -> e_world_context"
  - "[produces] e_classicist -> e_ontology_lead"
  - "[produces] e_dev -> e_pipeline_engineer"
  - "[produces] e_director -> e_world_builder"

timeline:
  - id: "t_monday"
    description: "Monday Morning Execution"
    scenes:
      - id: "sc_classicist_action"
        description: "ACTION: Email Classicist for CSV by Friday"
        entities: ["e_classicist", "e_csv_deliverable"]
      - id: "sc_dev_action"
        description: "ACTION: Email Dev - textures not animation"
        entities: ["e_dev", "e_texture_assets"]
      - id: "sc_director_action"
        description: "ACTION: Director defines camera stance"
        entities: ["e_director", "e_world_context"]
      - id: "sc_job_cards"
        description: "OPTIONAL: Generate formal job description cards"
        entities: ["e_ontology_lead", "e_pipeline_engineer", "e_world_builder"]

